<Page x:Class="GNPX_space.Func_Extensions"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GNPX_space"
      mc:Ignorable="d" 
      d:DesignWidth="572" d:DesignHeight="428"
      Title="Page1"
      Loaded="Page_Loaded" >

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/RoundCornerButtonStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    
    <Grid Width="572" Margin="0" Background="#FF050816">
        <Button x:Name="btn_DevelopWHS" Click="btn_Page_Clicked"
            Margin="29,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="60" Height="24"
            Content="WDF"
            Foreground="White" BorderBrush="#FF160F7A" FontSize="12" >
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF9EA8ED" Offset="1"/>
                    <GradientStop Color="#FF9EA8ED" Offset="0"/>
                    <GradientStop Color="#FF0C1133" Offset="0.5"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Button x:Name="btn_DevelopStatistics" Click="btn_Page_Clicked"
            Margin="94,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="60" Height="24"
            Content="Statistics"
            Foreground="White" BorderBrush="#FF160F7A" FontSize="12" >
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF9EA8ED" Offset="1"/>
                    <GradientStop Color="#FF9EA8ED" Offset="0"/>
                    <GradientStop Color="#FF0C1133" Offset="0.5"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>

        <Grid Background="#FF171A31" Margin="10,40,10,10">
            <Frame x:Name="frame_GNPX_Solve"  NavigationUIVisibility="Hidden" />
        </Grid>
    </Grid>


</Page>
