<Window x:Name="GNPXwinSub" 
        x:Class="GNPX_space.GNPX_winSub"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended" 
        xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

        xmlns:properties="clr-namespace:GNPX_space.Properties"
        xmlns:app="clr-namespace:GNPX_space"
        mc:Ignorable="d"    
        
        xmlns:GIDOO_space="clr-namespace:GIDOO_space"     
        
        Title="GNPXv6" Background="{x:Null}" WindowStyle="None"
        Width="350" Height="400"
        HorizontalAlignment="Left" VerticalAlignment="Top"
        ShowInTaskbar="False"
        WindowStartupLocation="Manual" BorderBrush="#FF2D20EF"
        MouseDown="Window_MouseDown" Loaded="GNPXwinSub_Loaded" >

    <Grid x:Name="GNPXv6" Background="#FF1D2752"  >
        <Label Content="GNPX" HorizontalAlignment="Left" Margin="11,5,0,0" VerticalAlignment="Top" Width="76" FontSize="26"
               Foreground="#FFB4ABFF" Background="{x:Null}"/>
        <Label Content="Final State" HorizontalAlignment="Right" Height="33" VerticalAlignment="Top" 
               FontFamily="MS Gothic" FontSize="20" FontWeight="Bold" Foreground="White" FontStyle="Italic" Margin="0,15,10,0"/>
        <Image x:Name="imgPB_GBoard" 
               Width="338" Height="338" Stretch="Fill" Source="LockedSet3D.png" Margin="1,0,2,8" VerticalAlignment="Bottom" />

    </Grid>

</Window>